<%- include("../partials/header"); %>

<div class="main-body">

<div class="panel-container-2c">
    <div class="panel-containee panel-border">
        <div class="title">
            <p>Profile</p>
        </div>
        <div class="content">
            <p class="ptitle"><%- entry.exchangeOverview.Name %></p>
            <p class="paragraph"><%- entry.exchangeOverview.Description %></p>
            <p class="ptitle">Industry</p>
            <p class="paragraph"><%- entry.exchangeOverview.Industry %></p>
            <p class="ptitle">Sector</p>
            <p class="paragraph"><%- entry.exchangeOverview.Sector %></p>
            <p class="ptitle">Currency</p>
            <p class="paragraph"><%- entry.exchangeOverview.Currency %></p>    
        </div>
    </div>
    <div class="panel-containee panel-border">
        <div class="title">
            <h2>Summary</h2>
        </div>
        <div class="content">
            <div class="panel-container-2c">
                <div class="panel-container-2c highlight" style="grid-column: 1 / -1">
                    <div class="panel-containee left">
                        <p class="emphasize-plusplus"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeQuote.Price %></span></p>
                        <p>Last update: <span class="format-date"><%- entry.exchangeQuote.LastTradingDay %></span></p>
                    </div>
                    <div class="panel-containee right">
                        <p class="emphasize-plusplus"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeQuote.Change %></span></p>
                        <p><span class="format-percentage"><%- entry.exchangeQuote.ChangePercent %></span></p>                
                    </div>
                </div>
                <div class="panel-container-2c">
                    <div class="panel-containee left">
                        <p class="ptitle nowrap">High</p>
                        <p class="ptitle nowrap">Low</p>
                        <p class="ptitle nowrap">Previous Close</p>
                        <p class="ptitle nowrap">Open</p>
                        <p class="ptitle nowrap">P/E</p>
                        <p class="ptitle nowrap">Forward P/E</p>
                        <p class="ptitle nowrap">Volume</p>
                        <p class="ptitle nowrap">Dividend</p>
                        <p class="ptitle nowrap">Dividend Date</p>
                        <p class="ptitle nowrap">Dividend Yield</p>
                        <p class="ptitle nowrap">Dividend Payout</p>                            
                    </div>
                    <div class="panel-containee right">
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeQuote.High %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeQuote.Low %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeQuote.PreviousClose %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeQuote.Open %></span></p>
                        <p class="nowrap"><span class="format-3-fraction-digits"><%- entry.exchangeOverview.PERatio %></span></p>
                        <p class="nowrap"><span class="format-3-fraction-digits"><%- entry.exchangeOverview.ForwardPE %></span></p>
                        <p class="nowrap"><span class="format-integer-short"><%- entry.exchangeQuote.Volume %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeOverview.Dividend %></span></p>
                        <p class="nowrap"><span class="format-date"><%- entry.exchangeOverview.DividendDate %></span></p>
                        <p class="nowrap"><span class="format-percentage"><%- entry.exchangeCalculated.DividendYieldPercent %></span></p>
                        <p class="nowrap"><span class="format-percentage"><%- entry.exchangeCalculated.DividendPayoutRatioPercent %></span></p>                            
                    </div>
                </div>
                <div class="panel-container-2c">
                    <div class="panel-containee left">
                        <p class="ptitle nowrap">52-Week High</p>
                        <p class="ptitle nowrap">52-Week Low</p>
                        <p class="ptitle nowrap">52-Week Range</p>
                        <p class="ptitle nowrap">Market Cap</p>
                        <p class="ptitle nowrap">EPS</p>
                        <p class="ptitle nowrap">Book value</p>
                        <p class="ptitle nowrap">Price/Book</p>
                        <p class="ptitle nowrap">Price/Sales</p>
                        <p class="ptitle nowrap">Target Price</p>
                        <p class="ptitle nowrap">Profit Margin</p>
                        <p class="ptitle nowrap">Shares</p>
                    </div>
                    <div class="panel-containee right">
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeOverview.Week52High %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeOverview.Week52Low %></span></p>
                        <p class="nowrap"><span class="format-percentage"><%- entry.exchangeCalculated.Week52RangePercent %></span></p>
                        <p class="nowrap"><span class="format-integer-short"><%- entry.exchangeOverview.MarketCapitalization %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeOverview.EPS %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeOverview.BookValue %></span></p>
                        <p class="nowrap"><span class="format-3-fraction-digits"><%- entry.exchangeOverview.PriceToBookRatio %></span></p>
                        <p class="nowrap"><span class="format-3-fraction-digits"><%- entry.exchangeOverview.PriceToSalesRatioTTM %></span></p>
                        <p class="nowrap"><span class="format-currency-<%- entry.exchangeOverview.Currency %>"><%- entry.exchangeOverview.TargetPrice %></span></p>
                        <p class="nowrap"><span class="format-percentage-raw"><%- entry.exchangeOverview.ProfitMargin %></span></p>
                        <p class="nowrap"><span class="format-integer-short"><%- entry.exchangeOverview.SharesOutstanding %></span></p>                                
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

</div>

<%- include("../partials/footer"); %>
<%- include("../partials/formatters"); %>