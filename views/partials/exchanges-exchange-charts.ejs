<% if (exItem) { %>
    <div class="panel-containee panel-border">
        <div class="content">

            <div id="timeline"
                data-chart-line
                data-W1='<%- JSON.stringify(exItem.exchangeGraphData.W1.map(ts => { return {x:ts.x,y:ts.y}; })) %>'
                data-M1='<%- JSON.stringify(exItem.exchangeGraphData.M1.map(ts => { return {x:ts.x,y:ts.y}; })) %>'
                data-M3='<%- JSON.stringify(exItem.exchangeGraphData.M3.map(ts => { return {x:ts.x,y:ts.y}; })) %>'
                data-M6='<%- JSON.stringify(exItem.exchangeGraphData.M6.map(ts => { return {x:ts.x,y:ts.y}; })) %>'
                data-Y1='<%- JSON.stringify(exItem.exchangeGraphData.Y1.map(ts => { return {x:ts.x,y:ts.y}; })) %>'
                data-Y5='<%- JSON.stringify(exItem.exchangeGraphData.Y5.map(ts => { return {x:ts.x,y:ts.y}; })) %>'
                >
            </div>
            
            <div class="tab">
                <button class="tablinks" onclick="changeChartData(this, 'timeline', 'W1')">1W</button>
                <button class="tablinks active" onclick="changeChartData(this, 'timeline', 'M1')">1M</button>  <!-- default -->
                <button class="tablinks" onclick="changeChartData(this, 'timeline', 'M3')">3M</button>
                <button class="tablinks" onclick="changeChartData(this, 'timeline', 'M6')">6M</button>
                <button class="tablinks" onclick="changeChartData(this, 'timeline', 'Y1')">1Y</button>
                <button class="tablinks" onclick="changeChartData(this, 'timeline', 'Y5')">5Y</button>
            </div>
            
        </div>
    </div>
<% } %>
