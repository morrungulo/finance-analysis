<!-- requires exchanges item (stock) -->

<% if(stock) { %>
    <div class="panel-containee panel-border">
        <div class="content">

            <div id="exchangeDaily" data-chart-line='<%- JSON.stringify(entry.exchangeXYDaily) %>'></div>
            
            <div class="tab">
                <button class="tablinks" onclick="changeChartData(this, 'exchangeDaily', '1W')">1W</button>
                <button class="tablinks active" onclick="changeChartData(this, 'exchangeDaily', '1M')">1M</button>
                <button class="tablinks" onclick="changeChartData(this, 'exchangeDaily', '3M')">3M</button>
                <button class="tablinks" onclick="changeChartData(this, 'exchangeDaily', '6M')">6M</button>
                <button class="tablinks" onclick="changeChartData(this, 'exchangeDaily', '1Y')">1Y</button>
                <button class="tablinks" onclick="changeChartData(this, 'exchangeDaily', '5Y')">5Y</button>
                <!-- <button class="tablinks" onclick="changeChartData(this, 'exchangeDaily', '10Y')">10Y</button> -->
            </div> 
        </div>
    </div>
<% } %>
