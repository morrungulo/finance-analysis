<!-- requires exchanges item (stock) -->

<% if(stock) { %>
    <div class="panel-containee panel-border">
        <div class="content">

            <div style="position: relative; width: 100%;">
                <canvas id="mychart" data-chart-line='<%- JSON.stringify(data) %>'></canvas>
            </div>
            
            <div class="tab">
                <button class="tablinks" onclick="selectChartData(this, 'mychart', '1W')">1W</button>
                <button class="tablinks active" onclick="selectChartData(this, 'mychart', '1M')">1M</button>
                <button class="tablinks" onclick="selectChartData(this, 'mychart', '3M')">3M</button>
                <button class="tablinks" onclick="selectChartData(this, 'mychart', '6M')">6M</button>
                <button class="tablinks" onclick="selectChartData(this, 'mychart', '1Y')">1Y</button>
                <button class="tablinks" onclick="selectChartData(this, 'mychart', '5Y')">5Y</button>
            </div> 
        </div>
    </div>
<% } %>
