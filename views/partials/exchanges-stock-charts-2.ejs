<!-- requires exchanges item (stock) -->

<% if(stock) { %>
    <div class="panel-containee panel-border">
        <div class="content">

            <div id="mychart2" data-chart-line-2='<%- JSON.stringify(entry.exchangeXYDaily) %>'></div>
            
            <div class="tab">
                <button class="tablinks" onclick="changeChartData(this, 'mychart2', '1W')">1W</button>
                <button class="tablinks active" onclick="changeChartData(this, 'mychart2', '1M')">1M</button>
                <button class="tablinks" onclick="changeChartData(this, 'mychart2', '3M')">3M</button>
                <button class="tablinks" onclick="changeChartData(this, 'mychart2', '6M')">6M</button>
                <button class="tablinks" onclick="changeChartData(this, 'mychart2', '1Y')">1Y</button>
                <button class="tablinks" onclick="changeChartData(this, 'mychart2', '5Y')">5Y</button>
                <button class="tablinks" onclick="changeChartData(this, 'mychart2', '10Y')">10Y</button>
            </div> 
        </div>
    </div>
<% } %>
